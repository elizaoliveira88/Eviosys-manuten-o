{
  "name": "FleetPlan",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "startNode": "node --http-parser=legacy ./node_modules/@angular/cli/bin/ng serve --port 4444 --host 127.0.0.1 --disable-host-check true",
    "start": "ng serve --port 4444 --disable-host-check true",
    "start:dev": "ng serve --port 4444 --proxy-config proxy.conf.json -c dev",
    "go": "ng serve --public-host http://localhost:4434/ --disable-host-check true",
    "docker-build": "ng build --configuration production --cross-origin 'use-credentials' --output-path 'www'",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prebuild": "npm --no-git-tag-version version patch",
    "extract-i18n": "ngx-translate-extract --input ./src --output ./src/assets/i18n/ --clean --sort --format namespaced-json",
    "mock": "json-server --watch mockServices.json",
    "test-offline": "http-server -p 8080 -c-1 dist",
    "copy-offline": "mkdir -p dist/lss/pilot && cp -R dist/D97/ dist/lss/pilot",
    "wtf": "lsof -i :4444",
    "setBuildTime": "date -u +\"%s\" > src/assets/timestamp.txt",
    "copyIndexFile": "cp -av dist/D97/index.html dist/D97/index.htm"
  },
  "private": true,
